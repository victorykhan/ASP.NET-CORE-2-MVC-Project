<link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {

            $(function () {
                $("#dialog-confirm").dialog({
                    resizable: false,
                    width: 600,
                    modal: true,
                    buttons: {
                        "No Thanks": function () {
                            $(this).dialog("close");
                        },
                        Yes: function () {
                            $(this).dialog("close");
                        }
                    }
                });
            });


        });
    </script>

    <script src="~/js/parallax.js"></script>


    <script type="text/javascript">
        $(document).ready(function (e) {
            if ($('.parallax-window').length) {
                $('.parallax-window').parallax();

            }

        });
        $('.carousel').carousel();

    </script>

    <script type="text/javascript">



        function LoadRecentPresidents() {
            $.ajax
                ({
                    url: "/Presidents/GetRecentPresidents",
                    contentType: "application/html; charset=utf-8",
                    type: "GET",
                    cache: !0,
                    datatype: "html",
                    success: function (t) {
                        $("#dvrecentpresi").html(t)
                    },
                    error: function () {
                        $("#dvrecentpresi").html("Data  Not Found")
                    }
                })
        }
        function LoadLongestPresidents() {
            $.ajax
                ({
                    url: "/Presidents/GetLongestPresidents",
                    contentType: "application/html; charset=utf-8",
                    type: "GET",
                    cache: !0,
                    datatype: "html",
                    success: function (t) {
                        $("#dvlongpresi").html(t)
                    },
                    error: function () {
                        $("#dvlongpresi").html("Data  Not Found")
                    }
                })
        }
        function LoadSenatElections() {
            $.ajax
                ({
                    url: "/Elections/GetSenateElections",
                    contentType: "application/html; charset=utf-8",
                    type: "GET",
                    cache: !0,
                    datatype: "html",
                    success: function (t) {
                        $("#dvsenat").html(t)
                    },
                    error: function () {
                        $("#dvsenat").html("Data  Not Found")
                    }
                })
        }

        function LoadPresiElections() {
            $.ajax
                ({
                    url: "/Elections/GetPresidentElections",
                    contentType: "application/html; charset=utf-8",
