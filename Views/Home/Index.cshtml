                        $("#dvparl").html("No upcoming Parliamentary elections")
                    }
                })
        }

        $(document).ready(function () {
            LoadSenatElections(), LoadPresiElections(), LoadCongElections(), LoadParlElections(), LoadLongestPresidents(), LoadRecentPresidents()
        });


    </script>


